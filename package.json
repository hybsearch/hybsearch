{
  "name": "@hybsearch/hybsearch",
  "private": true,
  "version": "4.0.0-rc.3",
  "description": "Find nonmonophyly in genetic sequences",
  "repository": "hybsearch/hybsearch",
  "homepage": "https://hybsearch.github.io/",
  "main": "main.js",
  "license": "MIT",
  "author": "The Hybsearch Team <hybsearch@stolaf.edu>",
  "scripts": {
    "server": "cd server && env NODE_ENV=production ./server.js",
    "build": "parcel build --detailed-report --out-dir ./server/ui/out --public-url /app ./server/ui/index.html",
    "clean": "rm -rf server/ui/out/",
    "start": "parcel --out-dir out --public-url /app ./server/ui/index.html",
    "test": "jest",
    "lint": "eslint --cache \"server/**/*.js\" \"server/**/hyb-*\"",
    "pretty-check": "npm run prettify -- --list-different",
    "p": "pretty-quick",
    "prettify": "prettier --write \"server/**/*.{js,css}\" \"server/**/hyb-*\""
  },
  "prettier": {
    "overrides": [
      {
        "files": "server/**/hyb-*",
        "options": {
          "parser": "babel"
        }
      }
    ],
    "useTabs": true,
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true
  },
  "jest": {
    "verbose": true
  },
  "browserslist": [
    "last 1 Firefox version",
    "last 1 Chrome version",
    "last 1 Safari version",
    "last 1 Edge version"
  ],
  "dependencies": {
    "@koa/cors": "2",
    "combinations-generator": "^1.0.1",
    "comma-separated-values": "^3.6.4",
    "d3": "^3.5.17",
    "dedent": "^0.7.0",
    "execa": "^1.0.0",
    "get-stdin": "^7.0.0",
    "hash-obj": "^1.0.0",
    "hasha": "^5.0.0",
    "json-stringify-safe": "^5.0.1",
    "koa": "^2.5.3",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static-server": "^1.4.0",
    "lodash": "^4.17.21",
    "loud-rejection": "^2.1.0",
    "make-dir": "^3.0.0",
    "meow": "^5.0.0",
    "minimist": "^1.2.2",
    "normalize.css": "^8.0.0",
    "parcel-bundler": "^1.12.4",
    "pretty-ms": "^5.0.0",
    "serialize-error": "^4.1.0",
    "slash-escape": "^1.0.0",
    "tempy": "^0.3.0",
    "word-wrap": "^1.2.3",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "expect": "^24.7.1",
    "jest": "^24.7.1",
    "jest-config": "^24.7.1",
    "jest-specific-snapshot": "^2.0.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.5.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.0",
    "utf-8-validate": "^5.0.1"
  }
}
